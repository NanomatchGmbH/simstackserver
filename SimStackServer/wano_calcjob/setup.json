{
    "name": "wano_calcjob",
    "author": "Nanomatch GmbH",
    "author_email": "timo.strunk@nanomatch.com",
    "description": "Wrapper to calculate Nanomatch WaNos (Workflow Active Nodes) as a CalcJob",
    "url": "https://github.com/timostrunk/wano_calcjob",
    "license": "proprietary",
    "classifiers": [
        "Programming Language :: Python",
        "Intended Audience :: Science/Industry",
        "License :: Proprietary",
        "Natural Language :: English",
        "Framework :: AiiDA :: SimStack"
    ],
    "version": "0.1.0a0",
    "entry_points": {
        "aiida.data": [
        ],
        "aiida.calculations": [
            "wano = wano_calcjob.calculations:DepositCalcJob"
        ],
        "aiida.parsers": [
            "wano = wano_calcjob.calculations.DepositParser"
        ],
        "aiida.cmdline.data": [
        ]
    },
    "include_package_data": true,
    "setup_requires": ["reentry"],
    "reentry_register": true,
    "install_requires": [
        "aiida-core>=1.1.0,<2.0.0",
        "six",
        "voluptuous"
    ],
    "extras_require": {
        "testing": [
            "pgtest~=1.3.1",
            "wheel~=0.31",
            "coverage",
            "pytest~=3.6,<5.0.0",
            "pytest-cov"
        ],
        "pre-commit": [
            "astroid==2.2.5",
            "pre-commit==1.20.0",
            "prospector==1.1.7",
            "pylint==2.3.1"
        ],
        "docs": [
            "sphinx",
            "sphinxcontrib-contentui",
            "sphinxcontrib-details-directive; python_version>='3.0'",
            "sphinx-rtd-theme"
        ]
    }
}
